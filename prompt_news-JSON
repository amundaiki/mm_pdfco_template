      "event_type": "[REQUIRED: FUNDING|EXPANSION|LEADERSHIP|PARTNERSHIP|PRODUCT_LAUNCH|IPO|M&A]",
      "event_date": "[REQUIRED: YYYY-MM-DD event date]", 
      "event_description": "[REQUIRED: 300-500 characters detailed event description]",
      
      "ceo_name": "[REQUIRED: Full name of CEO/Managing Director - research if needed]",
      "email": "[REQUIRED: Business email or estimate 'kontakt@bedriftsnavn.no']",
      "phone": "[REQUIRED: +47 XXXXXXXX format or 'Ikke tilgjengelig']",
      "website_url": "[REQUIRED: Full https:// URL or estimate 'https://www.bedriftsnavn.no']",
      
      "funding_amount_nok": "[REQUIRED: 'XXX M NOK'|'Ikke oppgitt']",
      "funding_round": "[REQUIRED: 'Serie A'|'Serie B'|'Serie C'|'Growth Capital'|'IPO'|'Operational']",
      
